{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File",
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "%BASEDIR%\\Logs\\Engine\\babrat-server-engine-processor-.log",
          "formatter": "Babrat.Server.Core.Logging.CustomCompactJsonFormatter, Babrat.Server.Core",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithThreadId",
      "WithExceptionDetails"
    ]
  },
  "ServerSettings": {
    "ListenAddress": "localhost",
    "ListenPort": 5056,
    "CertPath": "Cert\\server.pfx",
    "CertPassword": "123456"
  },
  "FileUploadSettings": {
    "MaxFileSize": 524288000
  },
  "Kafka": {
    "KafkaConsumerSettings" : {
      "ListenAddress": "localhost",
      "ListenPort": 9092,
      "GroupId" : "kafka-consumer-group",
      "AutoOffsetReset":"Latest"
    },
    "KafkaProducerSettings" : {
      "TargetAddress": "localhost",
      "TargetPort": 9092,
      "Acks" : "All",
      "MessageTimeoutMs" : 5000
    }
  },
  "Modules" : {
    "Grpc": {
      "AssemblyName": "Babrat.Server.Gateway.Grpc.dll",
      "Enabled": true
    },
    "GrpcImplementation": {
      "AssemblyName": "Babrat.Server.Gateway.GrpcImpl.dll",
      "Enabled": true
    },
    "MessageBusProducer" : {
      "AssemblyName" : "Babrat.Server.Gateway.MessageBus.Producer.dll",
      "Enabled" : true
    },
    "MessageBusConsumer" : {
      "AssemblyName" : "Babrat.Server.Gateway.MessageBus.Consumer.dll",
      "Enabled" : true
    }
  },
  "CreateTableServiceSettings" : {
    
  }

}



